@using MainProject.Core
@model string

<div class="page__nav-container">
	<div class="container">
		<div class="row no-gutters">
			<div class="col-lg-auto page__nav wow fadeInUp">
				<h2>@Html.GetResource(TempResourceKeyCollection.About)</h2>
				<ul class="nav__list lst-n">
					@foreach (var item in Model)
                    {
						<div class="nav__item"><a class="nav__link" href="@item.Link" target="@item.LinkTarget.ToString()">@item.Title</a></div>
                    }
				</ul>
			</div>
		</div>
	</div>
</div>